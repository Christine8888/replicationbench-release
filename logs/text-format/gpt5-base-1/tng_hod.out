│     # HOD models                                                             │
│                                                                              │
│     def hod_lrg_model(logM, log10_Mcut, log10_M1, sigma, alpha, kappa,       │
│ f_ic):                                                                       │
│         M = 10**logM                                                         │
│         Mcut = 10**log10_Mcut                                                │
│         M1 = 10**log10_M1                                                    │
│         from math import sqrt                                                │
│         # central                                                            │
│         x = (np.log10(Mcut/M)) / (np.sqrt(2)*sigma)                          │
│         from scipy.special import erfc                                       │
│         Ncen = 0.5 * f_ic * erfc(x)                                          │
│         # satellite                                                          │
│         Ns = np.clip((M - kappa*Mcut)/M1, a_min=0, a_max=None)               │
│         Nsat = (Ns**alpha) * Ncen                                            │
│         return Ncen, Nsat                                                    │
│                                                                              │
│     def fit_hod_lrg(logM_centers, Ncen_meas, Nsat_meas, n_trials=5000,       │
│ seed=42):                                                                    │
│         rng = np.random.default_rng(seed)                                    │
│         best = None                                                          │
│         best_chi = np.inf                                                    │
│         # Parameter ranges                                                   │
│         for _ in range(n_trials):                                            │
│             log10_Mcut = rng.uniform(12.0, 14.2)                             │
│             log10_M1 = rng.uniform(13.0, 15.5)                               │
│             sigma = rng.uniform(0.05, 0.8)                                   │
│             alpha = rng.uniform(0.5, 1.5)                                    │
│             kappa = rng.uniform(0.5, 2.0)                                    │
│             f_ic = rng.uniform(0.5, 1.0)                                     │
│             Ncen_pred, Nsat_pred = hod_lrg_model(logM_centers, log10_Mcut,   │
│ log10_M1, sigma, alpha, kappa, f_ic)                                         │
│             # Weight residuals; avoid bins where measurements are NaN        │
│             mask_c = np.isfinite(Ncen_meas)                                  │
│             mask_s = np.isfinite(Nsat_meas)                                  │
│             chi = 0.0                                                        │
│             if mask_c.any():                                                 │
│                 chi += np.mean((Ncen_meas[mask_c] - Ncen_pred[mask_c])**2)   │
│             if mask_s.any():                                                 │
│                 chi += np.mean((Nsat_meas[mask_s] - Nsat_pred[mask_s])**2)   │
│             if chi < best_chi:                                               │
│                 best_chi = chi                                               │
│                 best = (log10_Mcut, log10_M1, sigma, alpha, kappa, f_ic)     │
│         return best                                                          │
