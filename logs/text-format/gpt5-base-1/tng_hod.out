│     def measure_hod(dmo_hosts_all, centrals_flag, satellites_flag,           │
│ halos_dmo_m200c, host_group_indices, nbins=20):                              │
│         # halos_dmo_m200c in Msun/h units (float)                            │
│         # dmo_hosts_all: group index for each selected galaxy (length Ngals) │
│         # host_group_indices: array length N_halos giving group IDs          │
│ (0..Nhalo-1), in TNG it's 0..Nhalo-1 already                                 │
│         # Build per-host counts                                              │
│         # Count number of galaxies per host and centers per host             │
│         n_halos = halos_dmo_m200c.shape[0]                                   │
│         # Create arrays of zeros                                             │
│         ncen_per_host = np.zeros(n_halos, dtype=np.int32)                    │
│         nsat_per_host = np.zeros(n_halos, dtype=np.int32)                    │
│         # Aggregate                                                          │
│         # Using bincount on host indices                                     │
│         ncen_per_host += np.bincount(dmo_hosts_all[centrals_flag],           │
│ minlength=n_halos)                                                           │
│         nsat_per_host += np.bincount(dmo_hosts_all[satellites_flag],         │
│ minlength=n_halos)                                                           │
│         # Bin by mass                                                        │
│         logM = np.log10(halos_dmo_m200c)                                     │
│         finite_mask = np.isfinite(logM) & (halos_dmo_m200c > 0)              │
│         logM = logM[finite_mask]                                             │
│         ncen = ncen_per_host[finite_mask]                                    │
│         nsat = nsat_per_host[finite_mask]                                    │
│         # Define bins across mass range with halos present                   │
│         mmin = np.percentile(logM, 1)                                        │
│         mmax = np.percentile(logM, 99)                                       │
│         bins = np.linspace(mmin, mmax, nbins+1)                              │
│         bin_idx = np.digitize(logM, bins) - 1                                │
│         # Compute means per occupied bin                                     │
│         centers = 0.5*(bins[:-1]+bins[1:])                                   │
│         Nhalos_bin = np.bincount(bin_idx, minlength=nbins)                   │
│         Ncensum = np.bincount(bin_idx, weights=ncen, minlength=nbins)        │
│         Nsatmsum = np.bincount(bin_idx, weights=nsat, minlength=nbins)       │
│         with np.errstate(divide='ignore', invalid='ignore'):                 │
│             Ncen_mean = Ncensum / Nhalos_bin                                 │
│             Nsat_mean = Nsatmsum / Nhalos_bin                                │
│         valid = Nhalos_bin > 0                                               │
│         return centers[valid], Ncen_mean[valid], Nsat_mean[valid], bins      │
