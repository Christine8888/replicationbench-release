│     def select_LRG(grizy, delta_m=-0.4):                                     │
│         # grizy columns: g,r,i,z,y apparent absolute? Provided dataset per   │
│ example usage                                                                │
│         cg, cr, ci, cz, cy = grizy.T                                         │
│         # (r'-z') uses same shift so cancels on LHS; RHS uses z' dependence  │
│ (z + delta_m)                                                                │
│         mask = ( (cr - cz) > ( (cz + delta_m) - 16.83)*0.45 ) & \            │
│                ( (cr - cz) > ( (cz + delta_m) - 13.80)*0.19 )                │
│         return mask                                                          │
