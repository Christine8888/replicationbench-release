│     def minimum_image_delta(d, boxsize):                                     │
│         d = d - np.rint(d/boxsize)*boxsize                                   │
│         return d                                                             │
│                                                                              │
│     def projected_wp(positions, boxsize, rp_bins, pimax=80.0):               │
│         # positions: (N,3) in same units as box (here ckpc/h?). TNG outputs  │
│ positions in comoving kpc/h. Need Mpc/h.                                     │
│         # Convert from ckpc/h to cMpc/h                                      │
│         pos = positions / 1000.0                                             │
│         N = pos.shape[0]                                                     │
│         V = boxsize**3                                                       │
│         nbar = N / V                                                         │
│         # Generate pair counts in rp-pi bins                                 │
│         dpi = 2.0  # h^-1 Mpc bin width                                      │
│         pi_edges = np.arange(0.0, pimax + dpi + 1e-6, dpi)                   │
│         n_rp = len(rp_bins)-1                                                │
│         n_pi = len(pi_edges)-1                                               │
│         DD = np.zeros((n_rp, n_pi), dtype=np.int64)                          │
│         # Loop pairs - N is small (~few 1e3)                                 │
│         for i in range(N-1):                                                 │
│             d = pos[i+1:] - pos[i]                                           │
│             d[:,0] = minimum_image_delta(d[:,0], boxsize)                    │
│             d[:,1] = minimum_image_delta(d[:,1], boxsize)                    │
│             d[:,2] = minimum_image_delta(d[:,2], boxsize)                    │
│             rp = np.sqrt(d[:,0]**2 + d[:,1]**2)                              │
│             pi = np.abs(d[:,2])                                              │
│             # Select those within pi_max and rp range                        │
│             mask = (pi <= pimax) & (rp >= rp_bins[0]) & (rp <= rp_bins[-1])  │
│             if not np.any(mask):                                             │
│                 continue                                                     │
│             rp_sel = rp[mask]                                                │
│             pi_sel = pi[mask]                                                │
│             # Bin                                                            │
│             ir = np.digitize(rp_sel, rp_bins) - 1                            │
│             ip = np.digitize(pi_sel, pi_edges) - 1                           │
│             # Accumulate                                                     │
│             for rbin, pbin in zip(ir, ip):                                   │
│                 if 0 <= rbin < n_rp and 0 <= pbin < n_pi:                    │
│                     DD[rbin, pbin] += 1                                      │
│         # Compute xi(rp,pi) using RR for i<j pairs: RR = 0.5 * n^2 * V *     │
│ dV_bin, with dV_bin = 2*pi*rp*drp * 2*dpi                                    │
│         xi = np.zeros_like(DD, dtype=np.float64)                             │
│         for ir in range(n_rp):                                               │
│             rp_lo = rp_bins[ir]                                              │
│             rp_hi = rp_bins[ir+1]                                            │
│             drp = rp_hi - rp_lo                                              │
│             rp_mid = 0.5*(rp_lo + rp_hi)                                     │
│             shell_area = 2.0*np.pi*rp_mid*drp  # per unit length along pi    │
│             for ip in range(n_pi):                                           │
│                 dV = shell_area * (2.0 * dpi)  # account for +/- pi          │
│                 RR = 0.5 * (nbar**2) * V * dV                                │
│                 if RR > 0:                                                   │
│                     xi[ir, ip] = DD[ir, ip]/RR - 1.0                         │
│                 else:                                                        │
│                     xi[ir, ip] = np.nan                                      │
│         # Integrate to wp                                                    │
│         wp = 2.0 * np.nansum(xi * dpi, axis=1)                               │
│         # Return wp at rp bin centers                                        │
│         rp_centers = 0.5*(rp_bins[:-1]+rp_bins[1:])                          │
│         return rp_centers, wp                                                │
