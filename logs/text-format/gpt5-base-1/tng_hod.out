│     def classify_central_satellite(selected_hydro_sub_idx, hydro2dmo,        │
│ subhalos_dmo):                                                               │
│         # Map each selected hydro subhalo to DMO subhalo and then to DMO     │
│ host group                                                                   │
│         dmo_sub_idx = hydro2dmo[selected_hydro_sub_idx]                      │
│         valid_mask = dmo_sub_idx >= 0                                        │
│         selected_hydro_sub_idx = selected_hydro_sub_idx[valid_mask]          │
│         dmo_sub_idx = dmo_sub_idx[valid_mask]                                │
│         host = subhalos_dmo['SubhaloGrNr'][dmo_sub_idx]                      │
│         # For each host, find central as the one with max DMO subhalo mass   │
│         masses = subhalos_dmo['SubhaloMass'][dmo_sub_idx]                    │
│         # Build dict: host -> indices in the array                           │
│         centrals = np.zeros_like(selected_hydro_sub_idx, dtype=bool)         │
│         # We can vectorize by sorting by host and mass                       │
│         order = np.lexsort(( -masses, host ))                                │
│         host_sorted = host[order]                                            │
│         # Mark first occurrence per host as central                          │
│         first_of_host = np.r_[True, host_sorted[1:] != host_sorted[:-1]]     │
│         centrals_sorted = np.zeros_like(first_of_host, dtype=bool)           │
│         centrals_sorted[first_of_host] = True                                │
│         # Revert to original order                                           │
│         centrals[order] = centrals_sorted                                    │
│         satellites = ~centrals                                               │
│         return selected_hydro_sub_idx, dmo_sub_idx, host, centrals,          │
