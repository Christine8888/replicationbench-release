│     def select_ELG_with_scatter(grizy, seed=300, delta_m=0.6,                │
│ maglimits=(24.0,23.4,22.5)):                                                 │
│         rng = np.random.default_rng(seed)                                    │
│         # Use numpy global RNG as get_scatter uses np.random.normal;         │
│ temporarily set seed                                                         │
│         np.random.seed(seed)                                                 │
│         g_raw = grizy[:,0]                                                   │
│         r_raw = grizy[:,1]                                                   │
│         z_raw = grizy[:,3]                                                   │
│         g_s = get_scatter(g_raw, maglimits[0]) + delta_m                     │
│         r_s = get_scatter(r_raw, maglimits[1]) + delta_m                     │
│         z_s = get_scatter(z_raw, maglimits[2]) + delta_m                     │
│         # Cuts                                                               │
│         gr = g_s - r_s                                                       │
│         rz = r_s - z_s                                                       │
│         mask = (                                                             │
│             (g_s < 23.6) & (g_s > 20.0) &                                    │
│             (rz > 0.3) & (rz < 1.6) &                                        │
│             (gr < 1.15*rz - 0.15) & (gr < -1.2*rz + 1.6) &                   │
│             (g_s < maglimits[0]) & (r_s < maglimits[1]) & (z_s <             │
│ maglimits[2])                                                                │
│         )                                                                    │
│         return mask, g_s, r_s, z_s                                           │
